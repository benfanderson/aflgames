{
  "name": "aflgames",
  "version": "1.0.0",
  "description": "Using NPM, Express and the Squiggle AFL API to develop an app displaying the games of the shortened 2020 AFL season",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "build-babel": "babel -d ./dist ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./dist/index.js",
    "clean": "rm -rf dist && mkdir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benfanderson/aflgames.git"
  },
  "keywords": [
    "NPM",
    "javascript",
    "Express",
    "API"
  ],
  "author": "Ben Anderson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benfanderson/aflgames/issues"
  },
  "homepage": "https://github.com/benfanderson/aflgames#readme",
  "dependencies": {
    "@babel/preset-env": "^7.11.0",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "pug": "^3.0.0",
    "regenerator-runtime": "^0.13.7",
    "socket.io": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "eslint": "^7.7.0",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "14.8.0"
  }
}
