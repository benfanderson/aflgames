{
  "name": "aflgames",
  "version": "1.0.0",
  "description": "Using NPM, Express and the Squiggle AFL API to develop an app displaying the games of the shortened 2020 AFL season",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "nodemon ./dist/index.js",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benfanderson/aflgames.git"
  },
  "keywords": [
    "NPM",
    "javascript",
    "Express",
    "API"
  ],
  "author": "Ben Anderson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benfanderson/aflgames/issues"
  },
  "homepage": "https://github.com/benfanderson/aflgames#readme",
  "dependencies": {
    "express": "^4.17.1",
    "got": "^11.3.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "pug": "^3.0.0",
    "regenerator-runtime": "^0.13.7",
    "socket.io": "^2.3.0",
    "@babel/preset-env": "^7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "eslint": "^7.7.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "14.8.0"
  }
}
